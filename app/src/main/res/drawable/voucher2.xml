<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Blue line on the left -->
    <item
        android:left="0dp"
        android:right="10dp"
        android:top="0dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#00684E" />
        </shape>
    </item>

    <!-- White rectangle with rounded corners -->
    <item
        android:left="10dp"
        android:right="0dp"
        android:top="0dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <corners android:topRightRadius="10dp" android:bottomRightRadius="10dp" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>
